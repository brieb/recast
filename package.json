{
  "author": "Ben Newman <bn@cs.stanford.edu>",
  "name": "recast",
  "version": "0.16.1",
  "description": "JavaScript syntax tree transformer, nondestructive pretty-printer, and automatic source map generator",
  "keywords": [
    "ast",
    "rewriting",
    "refactoring",
    "codegen",
    "syntax",
    "transformation",
    "parsing",
    "pretty-printing"
  ],
  "homepage": "http://github.com/benjamn/recast",
  "repository": {
    "type": "git",
    "url": "git://github.com/benjamn/recast.git"
  },
  "license": "MIT",
  "main": "main.js",
  "types": "main.d.ts",
  "scripts": {
    "tsc": "tsc --noEmit",
    "mocha": "test/run.sh",
    "debug": "test/run.sh --inspect-brk",
    "test": "npm run tsc && npm run mocha",
    "clean": "script/run.sh clean",
    "build": "npm run build:dts && npm run build:js",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:js": "babel . --out-dir \".\" --extensions \".ts\" --ignore \"**/*.d.ts\",\"node_modules\",\"test/data\"",
    "prepack": "npm run clean && npm run build",
    "postpack": "npm run clean"
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "ast-types": "https://github.com/brieb/ast-types/releases/download/ts.alpha.0/ast-types-0.11.6-1.tgz",
    "esprima": "~4.0.0",
    "private": "~0.1.5",
    "source-map": "~0.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/parser": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/esprima": "^4.0.2",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.9",
    "babel-plugin-add-module-exports": "^1.0.0",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
    "flow-parser": "^0.83.0",
    "glob": "^7.1.2",
    "mocha": "~5.2.0",
    "reify": "^0.18.0",
    "typescript": "^3.1.6"
  },
  "engines": {
    "node": ">= 4"
  }
}
